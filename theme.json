{
	"$schema": "https://schemas.wp.org/wp/6.6/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#000000",
					"name": "Text",
					"slug": "text"
				},
				{
					"color": "#f1f1f1",
					"name": "Mid",
					"slug": "mid"
				},
				{
					"color": "#FFC300",
					"name": "Accent",
					"slug": "accent"
				}
			]
		},
		"custom": {
			"variation": "gws"
		},
		"dimensions": {
			"aspectRatios": [
				{
					"name": "Square - 1:1",
					"ratio": "1",
					"slug": "square"
				},
				{
					"name": "Standard - 4:3",
					"ratio": "4/3",
					"slug": "4-3"
				},
				{
					"name": "Portrait - 3:4",
					"ratio": "3/4",
					"slug": "3-4"
				},
				{
					"name": "Featured Image - 16:5",
					"ratio": "16/5",
					"slug": "16-5"
				},
				{
					"name": "Wide - 16:9",
					"ratio": "16/9",
					"slug": "16-9"
				}
			],
			"defaultAspectRatios": false
		},
		"layout": {
			"contentSize": "80%",
			"wideSize": "100%"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "1",
					"size": "1rem",
					"slug": "10"
				},
				{
					"name": "2",
					"size": "min(1.5rem, 2vw)",
					"slug": "20"
				},
				{
					"name": "3",
					"size": "min(2.5rem, 3vw)",
					"slug": "30"
				},
				{
					"name": "4",
					"size": "min(4rem, 5vw)",
					"slug": "40"
				},
				{
					"name": "5",
					"size": "min(6.5rem, 8vw)",
					"slug": "50"
				},
				{
					"name": "6",
					"size": "min(10.5rem, 13vw)",
					"slug": "60"
				}
			],
			"units": [
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": true,
			"defaultFontSizes": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Brockmann",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/webfonts/brockmann/brockmann-regular-webfont.woff2"
							]
						},
						{
							"fontFamily": "Brockmann Italic",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/webfonts/brockmann/brockmann-regularitalic-webfont.woff2"
							]
						},
						{
							"fontFamily": "Brockmann Medium",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/webfonts/brockmann/brockmann-medium-webfont.woff2"
							]
						},
						{
							"fontFamily": "Brockmann MediumItalic",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "500",
							"src": [
								"file:./assets/webfonts/brockmann/brockmann-mediumitalic-webfont.woff2"
							]
						},
						{
							"fontFamily": "Brockmann SemiBold",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/webfonts/brockmann/brockmann-semibold-webfont.woff2"
							]
						},
						{
							"fontFamily": "Brockmann SemiBoldItalic",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "600",
							"src": [
								"file:./assets/webfonts/brockmann/brockmann-semibolditalic-webfont.woff2"
							]
						},
						{
							"fontFamily": "Brockmann Bold",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/webfonts/brockmann/brockmann-bold-webfont.woff2"
							]
						},
						{
							"fontFamily": "Brockmann BoldItalic",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/webfonts/brockmann/brockmann-bolditalic-webfont.woff2"
							]
						}
					],
					"fontFamily": "\"Brockmann\", sans-serif",
					"name": "Brockmann",
					"slug": "main"
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "XX_Small",
					"size": "0.75rem",
					"slug": "xx-small"
				},
				{
					"fluid": false,
					"name": "Small",
					"size": "0.9rem",
					"slug": "small"
				},
				{
					"fluid": false,
					"name": "Normal",
					"size": "1rem",
					"slug": "normal"
				},
				{
					"fluid": false,
					"name": "Medium",
					"size": "1.5rem",
					"slug": "medium"
				},
				{
					"fluid": false,
					"name": "MediumLarge",
					"size": "1.85rem",
					"slug": "mediumlarge"
				},
				{
					"fluid": {
						"max": "3.25rem",
						"min": "1.39rem"
					},
					"name": "Large",
					"size": "3.85rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "5.5rem",
						"min": "1.2rem"
					},
					"name": "XX-Large",
					"size": "3.85rem",
					"slug": "xx-large"
				}
			],
			"letterSpacing": false,
			"lineHeight": true,
			"textDecoration": true,
			"textTransform": true,
			"writingMode": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"width": "0px",
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--accent)",
					"text": "var(--wp--preset--color--text)"
				},
				"spacing": {
					"padding": {
						"bottom": ".5rem",
						"left": "1rem",
						"right": "1rem",
						"top": ".5rem"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--main)",
					"fontWeight": "600",
					"textAlign": "left"
				}
			},
			"core/columns": {
				"border": {
					"radius": "1rem"
				},
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--60)"
				}
			},
			"core/heading": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--10)"
					}
				}
			},
			"core/image": {
				"border": {
					"radius": "0"
				}
			},
			"core/paragraph": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--10)"
					}
				}
			},
			"core/separator": {
				"border": {
					"width": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--text)"
				},
				"dimensions": {
					"minHeight": "2px"
				}
			},
			"core/quote": {
				"border": {
					"width": "0px",
					"radius": "0"
				},
				"color": {
					"text": "var(--wp--preset--color--text)"
				},
				"spacing": {
					"padding": {
						"bottom": "0",
						"left": "20vw",
						"right": "0",
						"top": "0"
					},
					"margin": {
						"bottom": "0",
						"left": "0",
						"right": "0",
						"top": "0"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--main)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "400",
					"textAlign": "left",
					"lineHeight": "1.3"
				}
			}
		},
		"color": {
			"text": "var(--wp--preset--color--text)"
		},
		"elements": {
			"h1": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--10)",
						"top": "0"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"fontWeight": "600",
					"lineHeight": "1.2!important"
				},
				"color": {
					"text": "var(--wp--preset--color--text)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "600",
					"lineHeight": "1"
				},
				"color": {
					"text": "var(--wp--preset--color--text)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "500",
					"lineHeight": "1"
				},
				"color": {
					"text": "var(--wp--preset--color--text)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-small)"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--accent)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--accent)"
				},
				"typography": {
					"textDecoration": "none"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--main)",
			"lineHeight": "1.5",
			"fontSize": "20px"
		}
	},
	"version": 3
}